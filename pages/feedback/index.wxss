/* pages/feedback/index.wxss */
.content {
  width: 100%;
  /* border: 1px solid red; */
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.upload-img {
  /* border: 1px solid red; */
  width: 100px;
  height: 100px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background-color: #f8f8f8;
}

.upload-img image {
  width: 100px;
  height: 100px;
}

.submit-btn {
  margin: 10px 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}